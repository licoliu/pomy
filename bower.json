{
  "group": "atom.lico",
  "artifact": "pomy",
  "name": "pomy",

  "version": "1.0.1",
  "title": "pomy - a software project management and comprehension tool",
  "description": "POMY is a software project management and comprehension tool. Based on the concept of a project object model (POM), Maven can manage a project's build, reporting and documentation from a central piece of information.",

  "main": "",
  "moduleType": [],
  "license": "MIT",
  "keywords": "POM, POMY, Maven, Surface, JSRT, Node.js, Gulp, Angularjs, Markdown, API Document, JsDoc, Open Source",
  "homepage": "https://github.com/licoliu/pomy",
  "private": false,
  "define": "cmd",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {},
  "devDependencies": {
    "jre": "~1.0.1"
  },
  "overrides": {
    "normalizeMulti": [{
      "dependencies": [
        "fontawesome",
        "bootstrap"
      ],
      "normalize": {
        "images": [
          "*.jpeg",
          "*.png",
          "*.jpg",
          "*.gif"
        ],
        "fonts": [
          "*.eot",
          "*.otf",
          "*.svg",
          "*.ttf",
          "*.woff",
          "*.woff2"
        ],
        "js": [
          "*.js"
        ],
        "css": [
          "*.css"
        ],
        "less": [
          "*.less"
        ]
      }
    }],
    "iscroll": {
      "main": [
        "build/iscroll-probe.js"
      ]
    }
  },
  "resolutions": {
    "angular": "~1.4.0"
  },
  "authors": [
    "lico <lico.atom@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/licoliu/pomy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:licoliu/pomy.git"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "scripts": {},
  "bin": {
    "pomy": "./bin/pomy.js"
  }
}
